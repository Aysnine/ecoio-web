    "micromatch": "^4.0.1",
    "mockjs": "^1.0.1-beta3",
    "mqtt": "^2.18.8",
    "nib": "^1.1.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.5.2",
